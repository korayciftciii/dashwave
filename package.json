{
    "name": "dashwave",
    "version": "1.2.0",
    "description": "A modern multi-tenant team collaboration platform built with Next.js, TypeScript, and Prisma",
    "author": {
        "name": "Koray Çiftçi",
        "email": "koray@example.com",
        "url": "https://github.com/korayciftciii"
    },
    "keywords": [
        "nextjs",
        "typescript",
        "prisma",
        "tailwindcss",
        "saas",
        "multi-tenant",
        "team-collaboration",
        "project-management",
        "task-management"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/korayciftciii/dashwave.git"
    },
    "homepage": "https://dashwave-six.vercel.app",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "db:migrate": "prisma migrate dev",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:push": "prisma db push",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:seed": "prisma db seed",
        "db:reset": "prisma migrate reset",
        "postinstall": "prisma generate",
        "clean": "rm -rf .next",
        "build:analyze": "ANALYZE=true npm run build",
        "preview": "npm run build && npm run start"
    },
    "dependencies": {
        "@clerk/nextjs": "^5.0.0",
        "@prisma/client": "^5.8.1",
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-collapsible": "^1.1.11",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-select": "^2.0.0",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-toast": "^1.1.5",
        "@types/nodemailer": "^6.4.17",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "critters": "^0.0.23",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.303.0",
        "moment": "^2.30.1",
        "next": "14.0.4",
        "nodemailer": "^7.0.3",
        "react": "^18",
        "react-big-calendar": "^1.19.4",
        "react-dom": "^18",
        "recharts": "^2.15.3",
        "sonner": "^1.3.1",
        "swr": "^2.3.3",
        "tailwind-merge": "^2.2.0",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-big-calendar": "^1.16.2",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "postcss": "^8",
        "prisma": "^5.8.1",
        "tailwindcss": "^3.3.0",
        "typescript": "^5"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    }
}